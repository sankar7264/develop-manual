{
  "name": "presidio-web",
  "version": "1.0.0",
  "description": "React web application",
  "author": "devops@t-3.com",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.14.0",
    "npm":  ">=7.0.0"
  },
  "scripts": {
    "install:from-lock": "npm install husky@$npm_package_devDependencies_husky && npm install --only=prod",
    "install:dev-from-lock": "npm install husky@$npm_package_devDependencies_husky && npm ci",
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "prettier": "prettier --write .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "typescript:global": "npm install -g typescript@$npm_package_dependencies_typescript"
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mui/material": "5.11.0",
    "@types/node": "^18.13.0",
    "axios": "1.2.3",
    "clsx": "1.2.1",
    "fs-extra": "11.1.0",
    "next": "12.3.4",
    "next-compose-plugins": "2.2.1",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sharp": "0.31.3",
    "tss-react": "4.6.0",
    "typescript": "^4.8.2"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@commitlint/cli": "17.4.0",
    "@commitlint/config-conventional": "17.4.0",
    "@emotion/server": "11.10.0",
    "@next/bundle-analyzer": "13.1.6",
    "@next/eslint-plugin-next": "13.1.0",
    "@storybook/addon-a11y": "6.5.16",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/react": "6.5.16",
    "@storybook/testing-library": "0.0.13",
    "@svgr/webpack": "6.5.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "12.1.5",
    "babel-loader": "8.3.0",
    "eslint": "8.29.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-node": "4.1.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.10",
    "husky": "8.0.0",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "storybook-addon-material-ui5": "1.0.0",
    "util": "0.12.5"
  },
  "resolutions": {
    "webpack": "^5",
    "**/**/trim": "^0.0.3",
    "@storybook/react/**/trim-newlines": "^3.0.1",
    "@storybook/react/**/glob-parent": "^5.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie >= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "not dead",
      "ie >= 11"
    ]
  }
}
